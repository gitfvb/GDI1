#reader(lib "htdp-intermediate-lambda-reader.ss" "lang")((modname gruppenuebung) (read-case-sensitive #t) (teachpacks ()) (htdp-settings #(#t constructor repeating-decimal #f #t none #f ())))
;; contract: recursive-fun :: (listof X) -> number
;; purpose: Berechnung der Länge der reingegebenen Liste

(define (recursive−fun problem) 
  (cond 
    [(trivially−solvable? problem) 
     (determine−solution problem)] 
    [else 
     (combine−solutions
      problem 
      (recursive-fun (generate−problem problem)))])) 

;; trivially-solvable
(define (trivially-solvable? problem)
  ()
  )

